server {
    listen         80;
    server_name    npe.marshallcodes.com www.npe.marshallcodes.com;

    location / {
		proxy_pass localhost:8001;
	}
}

server {
    listen         80;
    server_name    marshallcodes.com www.marshallcodes.com;

    location / {
		proxy_pass localhost:8000;
	}
}